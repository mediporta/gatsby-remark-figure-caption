{
  "name": "@mediporta/gatsby-remark-figure-caption",
  "description": "transform markdown image to figure with caption element",
  "homepage": "https://github.com/zielak/gatsby-remark-figure-caption",
  "version": "3.0.0",
  "author": "Leonardo Dino",
  "contributors": [
    {
      "name": "Darek Greenly",
      "url": "https://darekgreenly.com"
    }
  ],
  "keywords": [
    "gatsby",
    "image",
    "caption",
    "markdown",
    "remark",
    "gatsby-plugin",
    "remark"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "peerDependencies": {
    "gatsby": "^4.6.1",
    "gatsby-transformer-remark": "^5.6.0"
  },
  "repository": "https://github.com/mediporta/gatsby-remark-figure-caption",
  "scripts": {
    "build": "babel src --out-dir dist --ignore '**/*.test.js' -s",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "npm run build -w",
    "lint": "eslint src fixtures ",
    "jest": "jest",
    "test": "npm-run-all -s lint jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "bugs": {
    "url": "https://github.com/zielak/gatsby-remark-figure-caption/issues"
  },
  "devDependencies": {
    "@babel/runtime": "^7.16.7",
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.4.6",
    "cross-env": "^7.0.3",
    "core-js": "^2.6.12",
    "unist-util-visit": "^4.1.0",
    "eslint": "^8.8.0",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-react": "^7.28.0",
    "hast-util-assert": "^3.0.0",
    "hast-util-select": "^5.0.1",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-cli": "^27.4.7",
    "mdast-util-assert": "^4.0.0",
    "mdast-util-to-hast": "^12.1.1",
    "npm-run-all": "4.1.5",
    "remark": "^14.0.2",
    "unist-util-find": "^1.0.2",
    "unist-util-inspect": "^7.0.0",
    "unist-util-remove-position": "^4.0.1"
  },
  "jest": {
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "fixtures"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
